{
    "name": "whitecube/laravel-prices",
    "description": "Manage acquisition, selling & renting prices for products and services in your Laravel Application",
    "keywords": [
        "whitecube",
        "laravel",
        "prices",
        "e-commerce",
        "products",
        "services",
        "pricing",
        "history"
    ],
    "authors": [
        {
            "name": "Whitecube",
            "email": "hello@whitecube.be"
        },
        {
            "name": "Toon Van den Bos",
            "email": "toon@whitecube.be"
        },
        {
            "name": "Adrien Leloup",
            "email": "adrien@whitecube.be"
        }
    ],
    "require": {
        "whitecube/php-prices": "^3.0|^4.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Whitecube\\LaravelPrices\\": "src/",
            "Whitecube\\LaravelPrices\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Whitecube\\LaravelPrices\\LaravelPricesServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "orchestra/testbench": "^6.24|^7.0|^8.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
